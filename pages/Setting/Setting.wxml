<!--pages/Setting/Setting.wxml-->
<!-- 设置页面 -->
<view class="container"  style="width:{{windowWidth}}px;height:{{windowHeight}}px;">
    
  <view class='user-logo-view'>
    <view class='user-logo-img'>
      <image src=''></image>
    </view>
  </view>
  <view class='driver'></view>
  <view class='userCenter-setting-tools'>
    <view class='cell-setting-item top-radius bottom-driver arraw' bindtap="toWaterPriceSet">
      <text>水价设置</text>
    </view>
    <view class='cell-setting-item bottom-radius bottom-driver arraw' bindtap="toElectPriceSet">
      <text>电价设置</text>
    </view>
  </view>

</view>